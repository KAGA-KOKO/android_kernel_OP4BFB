/*VENDOR_EDIT motor start*/
/*Kun.Wang@PSW.BSP.Sensor 2019-05-22, Add for motor direction switch */
&pio {
    boost: boost {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO57__FUNC_GPIO57>;
            slew-rate = <1>;
            bias-pull-down; //pull down
            output-low; //digital output, no invert
            drive-strength = <2>;//0.15
        };
    };

    pwm_config: pwm_config {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO26__FUNC_PWM_0>;
            slew-rate = <1>;
            bias-disable;
            drive-strength = <0>;
        };
    };

    pwm_config_as_gpio: pwm_config_as_gpio {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO26__FUNC_GPIO26>;
            slew-rate = <1>;
            bias-disable;
            output-low;
            drive-strength = <0>; //LOW strength
        };
    };

    enable_gpio: enable_gpio {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO34__FUNC_GPIO34>;
            slew-rate = <1>;
            bias-disable; //No Pull
            output-low;
            drive-strength = <0>; //LOW strength
        };
    };

    dir_gpio: dir_gpio {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO33__FUNC_GPIO33>;
            slew-rate = <1>;
            bias-disable;
            output-low;
            drive-strength = <0>; /* LOW strength */
        };
    };

    standby_gpio: standby_gpio {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO30__FUNC_GPIO30>;
            slew-rate = <1>;
            bias-disable;
            output-low; /* digital output, no invert */
            drive-strength = <0>; /* LOW strength */
        };
    };

    mode1_gpio: mode1_gpio {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO149__FUNC_GPIO149>;
            slew-rate = <1>;
            bias-disable;
            output-low;
            drive-strength = <0>; /* LOW strength */
        };
    };
};

&motor_pl {
        status = "okay";
        compatible = "oppo-motor";
        hall-irq-pos = <170>;
	motor-retard-pos = <430>;
        delay-cycle = <12 14 20>; //ms
        slow-delay-cycle = <28 28 28>; //ms
        full-speed-param = <55 13500>;
        up-speed-param = <5 13500>;
        down-speed-param = <3 156250>;
        nodetect-speed-param = <15 13500>;
        slow-full-speed-param = <60 31250>;
        slow-up-speed-param = <5 104166>;
        slow-down-speed-param = <3 156250>;
        slow-nodetect-speed-param = <15 104166>;
        stop-neg-point = <2 2>;
        stop-pos-point = <2 2>;
        terminal-offset = <25 25>;
        offset-stop-retry = <2>;
	//pos-change-thrd = <300 350>;
	hall-data-sign = <0>;//1 represent positive ,0 represent negative
	press-irq-makeup = <55 55>;
	stop-retry = <4>;
	//motor_stop_val_support;
	//motor_stop_val = <2 1>;
	motor_down_stop_support;
	motor_down_stop_val = <1>;
	cal_stop_pos_val = <3 3>;
	cal_fix_stop_retry_support;
	cal_fix_stop_retry = <2>;
	motor_retard_offset_support;
	/*
	support mag noise decrease:
	511 - pattern[0] = irq_max
	pattern[1] - 512 = irq_min
	511 - pattern[2] = normal_max
	pattern[3] - 512 = normal_min
	average = 200
	*/
	mag_noise_support;
	mag_noise_pattern = <80 400 0 662 200 100 100>;
};

&motor_drv {
        status = "okay";
        compatible = "motor_drv-220";
        pinctrl-names = "boost","pwm_config","pwm_config_as_gpio","enable_gpio","dir_gpio","standby_gpio","mode1_gpio";
        pinctrl-0 = <&boost>;
        pinctrl-1 = <&pwm_config>;
        pinctrl-2 = <&pwm_config_as_gpio>;
        pinctrl-3 = <&enable_gpio>;
        pinctrl-4 = <&dir_gpio>;
        pinctrl-5 = <&standby_gpio>;
        pinctrl-6 = <&mode1_gpio>;
        boost-gpio = <&pio 57 0x00>;
        standby-gpio = <&pio 30 0>;
        dir-gpio = <&pio 33 0>;
        enable-gpio =  <&pio 34 0>;
        step-gpio = <&pio 26 0>;
        mode1-gpio = <&pio 149 0>;
        ratio-a = <24>;
        ratio-b = <1840>;//18.40
        ratio-c = <20>;
        ratio-d = <32>;
};

